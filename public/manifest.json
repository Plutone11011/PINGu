{

  "manifest_version": 2,
  "name": "PINGu",
  "version": "1.0",

  "description": "Pings visited website and computes RTT",

  "icons": {
    "48": "icons8-penguin-48.png",
    "96": "icons8-penguin-96.png"
  },

  "permissions": [
    "tabs",
    "webRequest",
    "<all_urls>"
  ],

  "browser_action": {
    "default_icon": "icons8-penguin-48.png",
    "default_title": "Ping",
    "default_popup": "index.html"
  }

}
